	rdfs:dataType rdfs:Literal ;
	rdfs:domain sctar:expression ;
	rdfs:range rdfs:Literal .

sctap:quotes
	rdfs:domain sctar:expression ;
	rdfs:range sctar:expression ;
	owl:inverseOf sctap:isQuotedBy .

